<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>
<?import javafx.geometry.Insets?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.java.tp.guiControllers.MainMenuController">
   <children>
      <Label styleClass="titulo-principal" text="Agencia de Viaje" />
      <Button fx:id="createTravelButton" text="Crear Viaje" onAction="#switchToCreateTravel"/>
      <Button fx:id="reportsButton" text="Reportes" onAction="#switchToReports"/>
      <Button fx:id="exitButton" text="Salir" onAction="#switchToExit"/>

   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
